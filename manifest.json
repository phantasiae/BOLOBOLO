{
    "manifest_version": 2,
    "name": "B",
    "version": "1.0",
    "description": "test B",
    "icons": {
        "48": "icons/beasts-48.png"
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "notifications",
        "storage",
        "tabs"
    ],
    "content_scripts": [
        {
          "matches": ["*://www.bilibili.com/v/*"],
          "js": ["list-blocker.js"]
        },
        {
            "matches": ["*://space.bilibili.com/*"],
            "js": ["user-marker.js"]
        }
    ],
    "background": {
        "scripts": ["background-script.js"]
    }
}